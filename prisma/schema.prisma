generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Menfess {
  id         Int       @id @default(autoincrement())
  sender     String
  message    String
  song       String?
  recipient  String
  spotify_id String?
  createdAt  DateTime  @default(now()) @map("created_at")
  updatedAt  DateTime? @updatedAt @map("updated_at")

  @@map("menfess")
}